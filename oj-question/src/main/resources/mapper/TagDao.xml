<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cheerful.oj.question.dao.TagDao">

    <resultMap type="com.cheerful.oj.question.entity.Tag" id="TagMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="tagName" column="tag_name" jdbcType="VARCHAR"/>
        <result property="gmtCreate" column="gmt_create" jdbcType="TIMESTAMP"/>
    </resultMap>

    <!-- 批量插入 -->
    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into oj_qms.qms_tag(tag_name, gmt_create)
        values
        <foreach collection="entities" item="entity" separator=",">
        (#{entity.tagName}, #{entity.gmtCreate})
        </foreach>
    </insert>
    <!-- 批量插入或按主键更新 -->
    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into oj_qms.qms_tag(tag_name, gmt_create)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.tagName}, #{entity.gmtCreate})
        </foreach>
        on duplicate key update
         tag_name = values(tag_name) , gmt_create = values(gmt_create)     </insert>

</mapper>

